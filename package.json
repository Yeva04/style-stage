{
  "name": "stylestage-sass",
  "version": "1.1.1",
  "description": "A minimal starter for Style Stage submissions that includes the source Sass used to create the base Style Stage theme, and the source HTML.",
  "main": "index.html",
  "scripts": {
    "scss": "npx sass --no-source-map sass:public",
    "watch": "npx sass --no-source-map --watch sass:public",
    "serve": "browser-sync start --server public --files public --port 1337",
    "start": "npx npm-run-all scss --parallel watch serve"
  },
  "keywords": [
    "style stage",
    "sass"
  ],
  "author": "5t3ph",
  "license": "ISC",
  "dependencies": {
    "browser-sync": "^3.0.4",
    "npm-run-all": "^4.1.5",
    "sass": "^1.26.10"
  },
  "devDependencies": {},
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Yeva04/style-stage.git"
  },
  "type": "commonjs",
  "bugs": {
    "url": "https://github.com/Yeva04/style-stage/issues"
  },
  "homepage": "https://github.com/Yeva04/style-stage#readme"
}
